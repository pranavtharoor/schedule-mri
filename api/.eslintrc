{
  "env": {
    "node": true,
    "es6": true,
    "jest": true
   },
  "extends": ["eslint:recommended", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": ["error", { "argsIgnorePattern": "next","ignoreRestSiblings": true }],
    "curly": ["error", "multi", "consistent"],
    "no-console": "off"
  }
}
